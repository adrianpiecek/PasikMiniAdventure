; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Pasik MiniAdventure"
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.4", "Mobile")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

Main="*res://Scripts/main.gd"
Global="*res://Scripts/Global.gd"
TransitionDiamond="*res://Scenes/transition_diamond.tscn"
TransitionManager="*res://Scripts/transition_manager.gd"
Music="*res://Scripts/effects/music.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"

[display]

window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Pasik MiniAdventure"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/ai_assistant_hub/plugin.cfg", "res://addons/debug_menu/plugin.cfg")

[global_group]

player=""
flying_platform=""

